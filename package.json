{
  "name": "FUTBoost",
  "version": "1.0.1",
  "description": "FUT Boost is the ultimate extension for the FUT Web App. The extension adds many functionality to simplify your life in the Web App, with tools like: rating filter, rarities filter, club summary, shortcuts actions, and more!",
  "author": "Gregory Wullimann",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.0",
    "lodash": "^4.17.19",
    "mousetrap": "^1.6.5",
    "popper.js": "^1.16.1",
    "shortcuts": "^1.3.2",
    "vue": "^2.6.10",
    "vuex": "^3.1.3",
    "vuex-map-fields": "^1.4.0",
    "vuex-persist": "^2.2.0",
    "vuex-persistedstate": "^3.0.1",
    "vuex-shared-mutations": "^1.0.2",
    "vuex-webextensions": "^1.3.0",
    "webextension-polyfill": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime-corejs3": "^7.6.3",
    "archiver": "^3.1.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.2.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "ejs": "^2.7.1",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.3.1",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.10",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.20.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.10"
  }
}
